select sum(sub1.ClicksA) + sum(sub2.ClicksB) AS TotalClicks
from 

  (
  select sum(ApplyButtonClickCount) As ClicksA, companypageid
  from JobApplyButtonClicks
  where date between '2019-01-01' and '2019-09-19'
  and companypageid in (14199, 20011)
  group by companypageid 
  ) 

sub1
left join

  (
  select sum(ApplyOutClickCount) As ClicksB, companypageid
  from JobApplyOutClick 
  where date between '2019-01-01' and '2019-09-19'
  and companypageid in (14199, 20011)
  group by companypageid
  )

sub2

on sub1.companypageid = sub2.companypageid 
