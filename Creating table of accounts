
--declaring rep
declare @rep char(40) = '##REP##'

declare @accounts table
(
id int
)


--insertion into table
******Must Be Fixed*****case when @rep = 'PANKIT' THEN insert into @accounts values (1), (2), (3), (4) ELSE null end




--Block of code
select DisplayName
from users
where Id in
(select Id from @accounts)
